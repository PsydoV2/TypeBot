{
  "name": "typebot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "dev": "nodemon --watch src -e ts --exec ts-node src/main.ts",
    "deployCommands": "npx ts-node src/scripts/deployCommands.ts",
    "deployAndStart": "npm run deployCommands && npm run start",
    "clearGlobals": "npx ts-node src/scripts/clearGlobals.ts",
    "clearDeployStart": "npm run clearGlobals && npm run deployAndStart"
  },
  "dependencies": {
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "private": true
}
